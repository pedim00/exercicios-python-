numeros = [987654321, 4, 7654321, 56, 1234567, 1, 88888, 3,
           42, 999999, 5, 1000000000, 13, 101010, 7, 444, 9, 2, 13, 9]


def buscar_numero(numero, lista):
    for elemento in lista:
        if elemento == numero:
            return True
    return False


numero_procurado = 56
encontrado = buscar_numero(numero_procurado, numeros)
if encontrado:
    print(f"O n√∫mero {numero_procurado} foi encontrado na lista.")
