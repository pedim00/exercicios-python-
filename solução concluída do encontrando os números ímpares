numeros = [987654321, 2, 7654321, 56, 1234567, 1, 88888, 3,
           42, 999999, 5, 1000000000, 13, 101010, 7, 444, 9, 2, 13, 9]


def somar_lista(lista):
    soma = 0
    for numero in lista:
        soma += numero
    return soma


def filtrar_impares(lista):
    impares = []
    for numero in lista:
        if numero % 2 != 0:
            impares.append(numero)
    return impares


impares = filtrar_impares(numeros)
print("NÃºmeros Ã­mpares encontrados na lista:", impares)
